#!/bin/sh

setsid -f mpv --referrer="https://gogoplay5.com" "$(curl -s -H "X-Requested-With:XMLHttpRequest" "https://gogoplay5.com/encrypt-ajax.php" -d "id=$(curl -s "https://gogoplay5.com/videos/$(curl -s "https://gogoplay5.com" | sed -nE 's_.*<a href="/videos/([^"]*)">_\1_p' | fzf)" | sed -nE 's/.*iframe src="([^"]*)".*/\1/p' | sed -nE 's/.*id=(.*)&title.*/\1/p' | openssl enc -e -aes256 -K '3633393736383832383733353539383139363339393838303830383230393037' -iv '34373730343738393639343138323637' | base64)" | sed -e 's/{"data":"//' -e 's/"}/\n/' -e 's/\\//g' | base64 -d | openssl enc -d -aes256 -K '3633393736383832383733353539383139363339393838303830383230393037' -iv '34373730343738393639343138323637' | sed -e 's/\].*/\]/' -e 's/\\//g' | tr "{|}" "\n" | sed -nE 's_\"file\":"(.*)",\"label.*_\1_p' | tail -1)"
